<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.wshuman3.weighttracker"
      android:versionName="1.2.3" android:versionCode="7">
      
    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="19"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    
    <application android:name="com.wshuman3.weighttracker.MyApplication" 
        android:icon="@drawable/ic_launcher_weighttracker"
        android:logo="@drawable/ic_menu_weighttracker" 
        android:label="@string/app_name"
        android:allowBackup="true">

	    <activity android:label="@string/app_name"
	          android:name="WeightTrackerActivity"
	          android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
	          <intent-filter>
	                <action android:name="android.intent.action.MAIN" />
	                <category android:name="android.intent.category.LAUNCHER" /> 
	            </intent-filter>
		</activity>

	    <activity android:name="GraphActivity"></activity>
		<activity android:name="WeighInActivity"></activity>
		<activity android:name="StatisticsActivity"></activity>
		<activity android:name="org.achartengine.GraphicalActivity"></activity>
	
	</application>
    


	

</manifest> 